{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "prettier/react",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "process": false
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "react/jsx-no-target-blank": "off",
        "react/react-in-jsx-scope": "off"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
